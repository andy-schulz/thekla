    "examples:01": "lerna run 01",
    "examples:02": "lerna run 02",
    "examples:10": "lerna run 10",
    "create:release:notes": "git log --pretty=\"%h - %s\" v2.5.0..HEAD --grep=FEATURE --grep=FIX --grep=REFACTORING"
