{
  "levels": {
    "HEADER": { "value": 19000, "colour": "magenta" }
  },
  "appenders": {
    "file": {
      "type": "dateFile",
      "category": "stream",
      "filename": "logs/process.log",
      "pattern": "-yyyy-MM-dd",
      "maxLogSize": 10485760,
      "numBackups": 3
    },
    "output": {
      "type": "stdout"
    }
  },
  "categories": {
    "default": {
      "appenders": ["output"],
      "level": "WARN" }
  }
}