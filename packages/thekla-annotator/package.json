{
  "name": "@thekla/annotator",
  "version": "2.0.0",
  "description": "helper functions to annotate web elements during testing sessions.",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "del-cli dist/**/* & tsc",
    "pretest": "tsc",
    "test": "jasmine --config=src/__test__/support/jasmine.json",
    "test:annotator": "jasmine --config=src/__test__/support/jasmine.json",
    "test:cov": "npm run pretest & nyc jasmine --config=src/__test__/support/jasmine.json",
    "pack": "npm pack",
    "clean:pack": "del-cli *.tgz"
  },
  "homepage": "https://andy-schulz.github.io/thekla/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andy-schulz/thekla.git"
  },
  "bugs": {
    "url": "https://github.com/andy-schulz/thekla/issues"
  },
  "author": "Andy Schulz",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@log4js-node/log4js-api": "^1.0.2",
    "webdriver": "^5.15.4"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@thekla/support": "^2.0.0",
    "del-cli": "^3.0.0",
    "nyc": "^14.1.1"
  },
  "gitHead": "953cabdef048e106a4ccfb463f2d904b2c5ea3f1"
